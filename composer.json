{
    "name": "symfony/skeleton",
    "type": "project",
    "license": "MIT",
    "description": "A minimal Symfony project recommended to create bare bones applications",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "^1.0@dev",
        "doctrine/annotations": "^1.12@dev",
        "doctrine/data-fixtures": "^1.4@dev",
        "doctrine/doctrine-bundle": "2.3.x-dev",
        "doctrine/doctrine-migrations-bundle": "3.1.x-dev",
        "doctrine/orm": "^v2.6.4",
        "nelmio/api-doc-bundle": "^3.0",
        "nyholm/psr7": "^1.3",
        "phpunit/phpunit": "^10.0@dev",
        "ramsey/uuid": "^4.0@dev",
        "ramsey/uuid-doctrine": "dev-master",
        "symfony/asset": "5.2.*",
        "symfony/console": "5.2",
        "symfony/dotenv": "5.2",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.2",
        "symfony/http-client": "5.2.*",
        "symfony/http-foundation": "5.2.*",
        "symfony/http-kernel": "5.2.*",
        "symfony/property-access": "5.2.*",
        "symfony/property-info": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/translation": "5.2.*",
        "symfony/twig-bundle": "5.2.*",
        "symfony/yaml": "5.2",
        "ext-http": "*",
        "ext-redis": "*"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^7.0@dev",
        "doctrine/doctrine-fixtures-bundle": "3.5.x-dev",
        "phpmetrics/phpmetrics": "dev-master",
        "symfony/maker-bundle": "^1.0@dev",
        "symfony/phpunit-bridge": "5.x-dev"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2"
        }
    }
}
